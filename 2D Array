CREATE A 2 D ARRAY;
#include<iostream>
using namespace std;

int main(){
    //create 2 d array 
    int arr[3][4];

      //taking input
    for(int i=0;i<3;i++){
        for(int j=0;j<4;j++){
            cin>> arr[i][j];
        }
    }
    //printing it 
    for(int i=0;i<3;i++){
        for(int j=0;j<4;j++){
            cout<< arr[i][j]<<" ";
        }
        cout<<endl;
    }
}
